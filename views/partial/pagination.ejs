<% if (total > 10) { %> <!-- 10 par page -->
    <div class="flex flex-col items-center mt-5">
    <span class="text-gray-900 dark:text-white">
      Affichage des pokemons <%- lowIndex %> Ã  <%- highIndex %> sur <%- total %>
    </span>
        <div class="inline-flex mt-2 text-[calc(4em)]">
            <a class="px-4 py-2 text-white bg-gray-800 rounded-l hover:bg-gray-900 aria-disabled:bg-gray-600 aria-disabled:text-gray-300 aria-disabled:cursor-not-allowed"
                <% if (previousPage === 0) { %> aria-disabled="true" <% } else { %> href="?page=<%- previousPage %>" <% } %> >
                <svg aria-hidden="true" class="w-5 h-5 mr-2" fill="currentColor" viewBox="0 0 20 20"
                     xmlns="http://www.w3.org/2000/svg">
